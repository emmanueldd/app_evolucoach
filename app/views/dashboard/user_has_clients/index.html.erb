<!--Popup-->
     <div class="popup-area bg-dark-blue full-vh white">
         <div class="container">
             <div class="row">
                 <div class="col-12">
                     <div class="d-flex justify-content-between align-items-center pop-header">
                         <a href="#">
                             <span class="left-angle d-none"></span>
                         </a>
                         <h1 class="font-light">FILTRER</h1>

                         <a id="close-popup" href="#">
                             <%= image_tag "cross.png", alt:"" %>
                         </a>
                     </div>


                     <div class="statut mb-60">
                         <p class="pop-title">Statut</p>
                         <div class="profile-radio popup">
                             <form action="#">
                                 <p>
                                     <input type="radio" id="client" name="radio-group">
                                     <label for="client">client</label>
                                 </p>
                                 <p>
                                     <input type="radio" id="prospect" name="radio-group" checked>
                                     <label for="prospect">prospect</label>
                                 </p>
                             </form>
                         </div>
                     </div>


                     <div class="sexe mb-60">
                         <p class="pop-title">Sexe</p>
                         <div class="profile-radio popup">
                             <form action="#">
                                 <p>

                                     <input type="radio" id="male" name="radio-group" checked>
                                     <label for="male"> <%= image_tag "male.png", alt:"" %></label>
                                 </p>
                                 <p>

                                     <input type="radio" id="female" name="radio-group">
                                     <label for="female"><%= image_tag "female.png", alt:"" %></label>
                                 </p>
                             </form>
                         </div>
                     </div>
                     <div class="age mb-10 mt-20">

                         <div class="age-title">
                             <p class="pop-title">
                                 Age
                             </p>
                             <p>
                                 <span id="min-price" class="slider-price">0</span> <span class="seperator">-</span> <span id="max-price" data-max="90" class="slider-price">90 ans</span>
                             </p>

                         </div>
                         <div class="selector">
                             <div class="price-slider">
                                 <div id="slider-range" class="ui-slider ui-corner-all ui-slider-horizontal ui-widget ui-widget-content">
                                     <div class="ui-slider-range ui-corner-all ui-widget-header"></div>
                                     <span tabindex="0" class="ui-slider-handle ui-corner-all ui-state-default"></span><span tabindex="0" class="ui-slider-handle ui-corner-all ui-state-default"></span>
                                 </div>

                             </div>
                         </div>


                         <div class="age-label">
                             <span>0</span>
                             <span>90</span>
                         </div>

                     </div>

                     <div class="ville mb-60">
                         <p class="pop-title">Ville</p>
                         <div class="header-seacrh">
                             <div class="form-group">
                                 <input type="seacrh" class="form-control" placeholder="Sélectionner une ville">
                                 <%= image_tag "search.png", alt:"Seaech", class:"search-icon" %>
                             </div>

                         </div>
                     </div>

                     <a href="#" class="popup-btn g-blue-bg">
                         OK
                     </a>

                 </div>
             </div>
         </div>
     </div>
