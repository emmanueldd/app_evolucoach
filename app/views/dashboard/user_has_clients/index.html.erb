<!--Content Area-->
<div class="content-area bg-light-blue full-vh page34 page15 page38">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <div class="content-inner page24">

          <% @clients.each do |client| %>
          <div class="member-box checkbox-item">
            <div class="member-left">

              <div class="member-check">

                <form action="#">
                  <p>
                    <input type="checkbox" id="user<%= client.id %>" name="radio-group">
                    <label class="checkbox-radio-style" for="user<%= client.id %>"></label>
                  </p>
                </form>
              </div>
              <div class="member-img">
                <%= image_tag client.infos.avatar_url , alt:"" %>
              </div>
              <div class="member-info">
                <h3><%= client.infos.first_name %> <%= client.infos.last_name %></h3>
                <p><%= client.infos.email %></p>
              </div>
            </div>
            <div class="member-right">
              <a href="<%= dashboard_user_has_client_path(client) %>">
                <% if client.is_client %>
                client
                <% else %>
                prospect
                <% end %>
                <span class="left-angle right"></span>
              </a>
            </div>
          </div>
          <% end %>
          <div class="add-item fixed">
            <a href="#" class="circle-btn grey-tik">
              <%= image_tag "grey-tik.png", alt:"" %>
            </a>
          </div>

        </div>

      </div>
    </div>
  </div>

</div>
<!--Content Area-->
